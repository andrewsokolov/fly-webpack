{
  "name": "fly-webpack",
  "version": "1.0.6",
  "description": "Webpack plugin for Fly.",
  "license": "MIT",
  "repository": "https://github.com/andrewsokolov/fly-webpack",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "LICENSE"
  ],
  "keywords": [
    "fly",
    "fly-plugin",
    "webpack",
    "automation",
    "pipeline",
    "promise",
    "async",
    "await",
    "task",
    "coroutine",
    "generator"
  ],
  "scripts": {
    "lint": "eslint src/*.js",
    "setup": "npm i && npm test",
    "build": "babel --optional runtime src -d dist",
    "test": "npm run build && npm run lint && npm run tape",
    "tape": "babel-node ./node_modules/tape/bin/tape test/*.js",
    "spec": "npm run test | tspec",
    "nyan": "npm run test | tnyan",
    "deploy": "npm run test && git push origin master && npm publish",
    "test-ci": "npm run spec"
  },
  "author": {
    "name": "Andrew Sokolov",
    "email": "asokolov@atlassian.com",
    "url": "http://github.com/andrewsokolov"
  },
  "dependencies": {
    "chalk": "^1.1.0",
    "webpack": "^1.10.5",
    "lodash": "^3.10.1",
    "memory-fs": "^0.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "eslint": "^0.21.2",
    "fly-util": "^4.0.1",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "tape-chai": "^1.1.0"
  },
  "engines": {
    "iojs": ">= 1.0.0",
    "node": ">= 0.11.0"
  }
}
